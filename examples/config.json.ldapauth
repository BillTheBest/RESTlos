{
    "nagios_main_cfg": "/etc/nagios/nagios.cfg",
    "nagios_bin": "/usr/sbin/nagios",
    "output_dir": "/etc/nagios/objects/api",
    "port": 5000,
    "sudo": false,
    "auth": {
        "provider": "AuthLDAP",
        "params": {
            "ldapserver": "ldap.example.com",
            "domain": "EXAMPLE.COM",
            "ssl": true,
            "groups": [ "admins", "developers" ]
        }
    },
    "logging": {
        "root": {
            "level": "DEBUG",
            "handlers": ["console", "syslog"],
            "propagate": true
        }
    }
}
